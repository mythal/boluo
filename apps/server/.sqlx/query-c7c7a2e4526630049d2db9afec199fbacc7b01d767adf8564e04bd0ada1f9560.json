{
  "db_name": "PostgreSQL",
  "query": "WITH nearest AS (\n    SELECT pos_p, pos_q\n    FROM messages\n    WHERE pos > $3::INT4::float8 / $4::INT4::float8 AND channel_id = $1\n    ORDER BY pos ASC\n    LIMIT 1\n), initial AS (\n    SELECT 1 AS pos_p, 0 AS pos_q\n), bottom AS (\n    SELECT COALESCE(nearest.pos_p, initial.pos_p) AS pos_p, COALESCE(nearest.pos_q, initial.pos_q) AS pos_q\n    FROM nearest RIGHT JOIN initial ON TRUE\n)\nUPDATE messages\nSET (pos_p, pos_q) = (select p as pos_p, q as pos_q from find_intermediate($3::INT4, $4::INT4, bottom.pos_p, bottom.pos_q))\nFROM bottom\nWHERE channel_id = $1 AND id = $2\nRETURNING messages as \"message!: Message\";\n",
