{
  "presets": [
    "@babel/env",
    "@babel/typescript",
    ["@babel/preset-react", { "runtime": "automatic", "importSource": "@emotion/react" }],
    [
      "@emotion/babel-preset-css-prop",
      {
        "autoLabel": "always",
        "labelFormat": "[local]"
      }
    ]
  ],
  "plugins": [["@emotion", { "sourceMap": true }]]
}
