{
  "name": "boluo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.3",
    "turbo": "^1.7.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "dprint": "^0.34.4",
    "typescript": "latest"
  },
  "packageManager": "pnpm@7.25.0"
}
